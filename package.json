{
  "name": "piano-tuning",
  "description": "Jordan's Piano Tuning Website",
  "version": "0.0.1",
  "homepage": "https://tseitz.github.io/PianoTuning",
  "license": "MIT",
  "scripts": {
    "dev": "sapper dev",
    "watch:tailwind": "postcss static/tailwind.css -o static/index.css -w",
    "build:tailwind": "SET NODE_ENV=production postcss static/tailwind.css -o static/index.css",
    "build": "npm run build:tailwind && sapper build",
    "deploy": "gh-pages -d __sapper__/build",
    "export": "sapper export",
    "start": "node __sapper__/build",
    "cy:run": "cypress run",
    "cy:open": "cypress open",
    "test": "run-p --race dev cy:run"
  },
  "dependencies": {
    "@fullhuman/postcss-purgecss": "^2.0.6",
    "compression": "^1.7.1",
    "gh-pages": "^2.2.0",
    "polka": "next",
    "sirv": "^0.4.0"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/runtime": "^7.0.0",
    "@rollup/plugin-commonjs": "^11.0.0",
    "@rollup/plugin-node-resolve": "^6.0.0",
    "@rollup/plugin-replace": "^2.2.0",
    "autoprefixer": "^9.7.4",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^7.1.0",
    "rollup": "^1.20.0",
    "rollup-plugin-babel": "^4.0.2",
    "rollup-plugin-svelte": "^5.0.1",
    "rollup-plugin-tailwindcss": "^1.0.0",
    "rollup-plugin-terser": "^4.0.4",
    "sapper": "^0.27.0",
    "svelte": "^3.0.0",
    "tailwindcss": "^1.2.0"
  }
}
